@model CorpModel
@{
	ViewData["Title"] = "Bind";
}

<h2>Input</h2>

<form action="/Print" method="get" class="form-horizontal">
	<div class="form-group">
		<label for="Document" class="col-xs-3 col-sm-2 control-label">Document: </label>
		<div class="col-xs-9 col-sm-10">
			<input id="Document" type="text" value="C:\Work\test.docx" name="p" class="form-control" placeholder="Input a file path">
		</div>
	</div>
	<div class="form-group">
		<label for="UserID" class="col-xs-3 col-sm-2 control-label">User ID: </label>
		<div class="col-xs-9 col-sm-10">
			<input id="UserID" type="text" value="@Model.UserId" name="u" class="form-control" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-xs-3 col-sm-2"></label>
		<div class="col-xs-9 col-sm-10">
			<button type="submit" class="btn btn-danger">Next</button>
		</div>
	</div>
</form>
