
@{
    ViewData["Title"] = "Settings";
    Layout = "_LayoutAdmin";
}

<h2>设置</h2>

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">系统</h3>
    </div>
    <div class="panel-body">
        <form>
            <div class="form-group">
                <label for="exampleInputEmail1">临时文件目录</label>
                <input type="text" class="form-control" id="exampleInputEmail1" placeholder="默认为系统临时文件目录 TEMP">
            </div>
        </form>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">uniFLOW</h3>
    </div>
    <div class="panel-body">
        <form>
            <div class="form-group">
                <label for="exampleInputEmail1">uniFLOW 服务器地址</label>
                <input type="text" class="form-control" id="exampleInputEmail1" placeholder="">
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">uniFLOW REST 服务秘钥</label>
                <input type="text" class="form-control" id="exampleInputEmail1" placeholder="">
            </div>
