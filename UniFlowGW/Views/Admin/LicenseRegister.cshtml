@{
    ViewData["Title"] = "注册授权码";
    Layout = "_LayoutAdmin";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-6">
        
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label for="key"></label>
                <input id="key" name="key" class="form-control" maxlength="25" required />
            </div>
            <button type="submit" class="btn btn-default">注册授权码</button>
        </form>
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
